{
	"name": "enjoy-net",
	"publisher": "enjoy-vscode-plugin",
	"displayName": "enjoy net",
	"description": "enjoy net by vscode",
	"version": "0.0.3",
	"repository": {
		"url": "https://github.com/MrZJD/enjoy-vscode-plugin.git"
	},
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:enjoy.start",
		"onCommand:enjoy.baidu",
		"onCommand:enjoy.v2ex",
		"onCommand:enjoy.cnode",
		"onCommand:enjoy.segmentfault"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "enjoy.start",
				"title": "enjoy: start enjoy service"
			},
			{
				"command": "enjoy.baidu",
				"title": "enjoy: baidu service"
			},
			{
				"command": "enjoy.v2ex",
				"title": "enjoy: v2ex service"
			},
			{
				"command": "enjoy.cnode",
				"title": "enjoy: cnode service"
			},
			{
				"command": "enjoy.segmentfault",
				"title": "enjoy: segmentfault service"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.14.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"babel-eslint": "^10.0.2",
		"cheerio": "^1.0.0-rc.3",
		"cross-fetch": "^3.0.4"
	}
}
